      .Add("****")
      .Add("****")
      .Add("****")
    End With
    $BillingReport.Add(asx)
    asx.Clear()

  Endif

  res3 = modDatabase.$syConn.Exec("select fldid,fldcategory,flditem,flddose,fldfreq from tblextradosing where fldencounterval=&1 and fldstatus=&2 and fldsave=&3", encid, "Continue", True)

''--------------tblpatbilldetail
Public Function GetIDInvoiceClearanceRange($conn As Connection, bilid1 As Long, bilid2 As Long, sUser As String, sComp As String, sLocaType As String, sLocation As String, Optional sBillDetail As String) As Float

  Dim res As Result
  Dim xval As Float
  Dim RepoStr As String

  Dim xbilldetail As String

  If sBillDetail Then
    xbilldetail = sBillDetail
  Else
    xbilldetail = "tblpatbilldetail"
  Endif
  RepoStr = modDataRepo.GetWhereStringRepo(sLocaType, sLocation)
  res = $conn.Exec("select SUM(fldreceivedamt) as totamt from " & xbilldetail & " where fldid>=&1 and fldid<=&2 and flditemamt=&3 and flduserid like &4 and fldcomp like &5" & RepoStr, bilid1, bilid2, 0, sUser, sComp)                                        ''
  If res.Available Then
    If res["totamt"] Then
      xval = res["totamt"]
    Else
      xval = 0
    Endif
  Else
    xval = 0
  Endif
  Return xval

End

''multi comp
Public Function GetIDInvoiceClearanceRangeMulti($conn As Connection, bilid1 As Long, bilid2 As Long, sUser As String, aCompLst As String[], sLocaType As String, sLocation As String, Optional sBillDetail As String) As Float

  Dim res As Result
  Dim xval As Float
  Dim RepoStr As String
  Dim i As Integer
  Dim sCompLst As String[]

  Dim xbilldetail As String

  If sBillDetail Then
    xbilldetail = sBillDetail
  Else
    xbilldetail = "tblpatbilldetail"
  Endif

  sCompLst = aCompLst.Copy()
  For i = 0 To sCompLst.Count - 1
    sCompLst[i] = "'" & sCompLst[i] & "'"
  Next
  RepoStr = modDataRepo.GetWhereStringRepo(sLocaType, sLocation)
  res = $conn.Exec("select SUM(fldreceivedamt) as totamt from " & xbilldetail & " where fldid>=&1 and fldid<=&2 and flditemamt=&3 and flduserid like &4 and fldcomp in(" & sCompLst.Join(",") & ")" & RepoStr, bilid1, bilid2, 0, sUser)                                        ''
  If res.Available Then
    If res["totamt"] Then
      xval = res["totamt"]
    Else
      xval = 0
    Endif
  Else
    xval = 0
  Endif

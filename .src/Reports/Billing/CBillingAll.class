Public Function GetInvoiceReportBillingRangeCashSource($BillingReport As CReportHTML, $con As Connection, bill1 As Variant, bill2 As Variant, sLocaType As String, sLocation As String) As Float

  Dim RepoVal As Float
  Dim bilid1 As Variant
  Dim bilid2 As Variant
  Dim xfld As String
  Dim res1 As Result
  Dim res3 As Result

  Dim asx As New String[0]
  Dim res As Result

  Dim xrecv As Float
  Dim urecv As Float
  Dim totrecv As Float

  Dim p As Integer
  Dim RepoStr As String

  RepoStr = modDataRepo.GetWhereStringRepo(sLocaType, sLocation)
  If MMain.$IsGUIApp = True Then
    $ProgressBar1 = modAppSupport.FindWorkProgressBar(modHelpVariable.$LogInCategory)
    $ProgressBar1.Tag = "Const"
  Endif

  bilid1 = bill1
  bilid2 = bill2
  xfld = "fldtime"

  If bilid1 And If bilid2 Then
    res1 = $con.Exec("select distinct(flduserid) as col from tblcashsources where " & xfld & ">=&1 and " & xfld & "<=&2" & GetLockString() & RepoStr, bilid1, bilid2)
    totrecv = 0

    p = 1
    For Each res1
      $BillingReport.AddSection(res1["col"] & " [" & modGeneral.GetUserFullName(res1["col"]) & "]", True)

      res = $con.Exec("select distinct(fldvendor) as col from tblcashsources where " & xfld & ">=&1 and " & xfld & "<=&2 and flduserid=&3" & GetLockString() & RepoStr, bilid1, bilid2, res1["col"])
      urecv = 0

      For Each res
        'for individual account
        $BillingReport.AddChapter(res["col"])
        res3 = $con.Exec("select fldtime,fldencounterval,fldencounterval,fldvendor,fldbillno,fldcashamt,fldreference from tblcashsources where " & xfld & ">=&1 and " & xfld & "<=&2 and flduserid like &3 and fldvendor like &4" & GetLockString() & RepoStr, bilid1, bilid2, res1["col"], res["col"])
        xrecv = 0

        If res3.Available Then
          For Each res3
            If res3["fldcashamt"] Then
              xrecv = xrecv + res3["fldcashamt"]
            Endif
            With asx
              .Add(modString.HTMLBlankSpace(3) & modReportVar.GetDateTimeReport(res3["fldtime"], gb.GeneralDate))
              .Add(res3["fldencounterval"])
              .Add(modPatient.GetPatientNameByEnc(res3["fldencounterval"], $con))
              .Add(res3["fldreference"])
              .Add(res3["fldbillno"])
              .Add("")
              .Add("")
              .Add("")
              .Add("")
              .Add(modReportVar.GetLocaleNumberFormat(res3["fldcashamt"], gb.Currency))
              .Add(res3["fldvendor"])
            End With
            $BillingReport.Add(asx)
            asx.Clear()
          Next
        Endif

        urecv = urecv + xrecv

        With asx
          .Add(modString.HTMLBlankSpace(3) & res1["col"] & "@" & res["col"])
          .Add("")
          .Add("")
          .Add("")
          .Add("")
          .Add("")
          .Add("")
          .Add("")
          .Add("")
          .Add(modReportVar.GetLocaleNumberFormat(xrecv, gb.Currency))
          .Add("")
        End With
        $BillingReport.Add(asx)
        asx.Clear()
        With asx
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
          .Add("....")
        End With
        $BillingReport.Add(asx)
        asx.Clear()
      Next

      totrecv = totrecv + urecv
      With asx
        .Add("<b>" & res1["col"] & " @ Total</b>")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add(modReportVar.GetLocaleNumberFormat(urecv, gb.Currency))
        .Add("")
      End With
      $BillingReport.Add(asx)
      asx.Clear()
      With asx
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
        .Add("")
      End With
      $BillingReport.Add(asx)
      asx.Clear()

      If MMain.$IsGUIApp = True Then
        $ProgressBar1.Value = p / res1.Count
        Wait
      Endif
      p = p + 1
    Next

    With asx
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add(modReportVar.GetLocaleNumberFormat(totrecv, gb.Currency))
      .Add("")
    End With
    $BillingReport.Add(asx)
    asx.Clear()
    GetFooterMsg($BillingReport, sLocaType, sLocation)

    Return RepoVal
  Endif

End

Public Function GetInvoiceReportGroupsSummarySpecial($BillingReport As CReportHTML, sFormat As String, $con As Connection, bill1 As Variant, bill2 As Variant, aList As String[], sLocaType As String, sLocation As String, $tblpatbilling As String, $tblpatbilldetail As String) As Float

  Dim RepoVal As Float
  Dim asx As New String[0]
  Dim bilid1 As Variant
  Dim bilid2 As Variant
  Dim xfld As String
  Dim grpname As String
  Dim grpList As String[]
  Dim res2 As Result
  Dim res3 As Result
  Dim xList As String[]
  Dim xcomp As String

  Dim usrdue As Float
  Dim usrtax As Float
  Dim usrdsc As Float
  Dim usrcrdt As Float
  Dim usrcash As Float

  Dim xdue As Float
  Dim xtax As Float
  Dim xdsc As Float
  Dim xcrdt As Float
  Dim xcash As Float

  Dim totclear As Float
  Dim totrnd As Float
  Dim totcrnd As Float

  Dim RepoStr As String
  Dim i As Integer

  xList = aList.Copy()
  For i = 0 To xList.Count - 1
    xList[i] = "'" & xList[i] & "'"
  Next

  RepoStr = modDataRepo.GetWhereStringRepo(sLocaType, sLocation, "t2")
  If sFormat = "Date" Then
    bilid1 = bill1
    bilid2 = bill2
    xfld = "t2.fldtime"
  Else
    bilid1 = modNonMedical.GetBillIDFromBillNo(bill1, $tblpatbilldetail)
    bilid2 = modNonMedical.GetBillIDFromBillNo(bill2, $tblpatbilldetail)
    xfld = "t2.fldid"
  Endif

  If bilid1 And If bilid2 Then
    grpList = modNonMedical.GetGroupNameAccount()
    usrdue = 0
    usrtax = 0
    usrdsc = 0
    usrcrdt = 0
    usrcash = 0

    ''if group name found
    For Each grpname In grpList
      res2 = $con.Exec("select " & modNonMedical.CashCreditBillSQL("t1") & " from " & $tblpatbilling & " AS t1 inner join " & $tblpatbilldetail & " AS t2 ON t1.fldbillno=t2.fldbillno where t1.flditemname in(select flditemname from tblreportgroup where fldgroup like &1) and " & xfld & ">=&2 and " & xfld & "<=&3 and t2.fldcomp in(" & xList.Join(",") & ")" & RepoStr, grpname, bilid1, bilid2)
      If res2.Available Then
        xdue = 0
        xtax = 0
        xdsc = 0
        xcrdt = 0
        xcash = 0
        If res2["duemt"] Then
          xdue = xdue + res2["duemt"]
        Endif
        If res2["disctot"] Then
          xdsc = xdsc + res2["disctot"]
        Endif
        If res2["taxtot"] Then
          xtax = xtax + res2["taxtot"]
        Endif
        If res2["xcredit"] Then
          xcrdt = xcrdt + res2["xcredit"]
        Endif
        If res2["xcash"] Then
          xcash = xcash + res2["xcash"]
        Endif

        usrdue = usrdue + xdue
        usrdsc = usrdsc + xdsc
        usrtax = usrtax + xtax
        usrcrdt = usrcrdt + xcrdt
        usrcash = usrcash + xcash

        With asx
          .Add(grpname)
          .Add(modReportVar.GetLocaleNumberFormat(xdue, gb.Currency))
          .Add(modReportVar.GetLocaleNumberFormat(xdsc, gb.Currency))
          .Add(modReportVar.GetLocaleNumberFormat(xtax, gb.Currency))
          .Add(modReportVar.GetLocaleNumberFormat(xcrdt, gb.Currency))
          .Add(modReportVar.GetLocaleNumberFormat(xcash, gb.Currency))
        End With
        $BillingReport.Add(asx)
        asx.Clear()
      Endif
    Next

    ''if group name not found
    res3 = $con.Exec("select " & modNonMedical.CashCreditBillSQL("t1") & " from " & $tblpatbilling & " AS t1 inner join " & $tblpatbilldetail & " AS t2 ON t1.fldbillno=t2.fldbillno where t1.flditemname not in(select flditemname from tblreportgroup) and " & xfld & ">=&1 and " & xfld & "<=&2 and t2.fldcomp in(" & xList.Join(",") & ")" & RepoStr, bilid1, bilid2)
    If res3.Available Then
      xdue = 0
      xtax = 0
      xdsc = 0
      xcrdt = 0
      xcash = 0
      If res3["duemt"] Then
        xdue = xdue + res3["duemt"]
      Endif
      If res3["disctot"] Then
        xdsc = xdsc + res3["disctot"]
      Endif
      If res3["taxtot"] Then
        xtax = xtax + res3["taxtot"]
      Endif
      If res3["xcredit"] Then
        xcrdt = xcrdt + res3["xcredit"]
      Endif
      If res3["xcash"] Then
        xcash = xcash + res3["xcash"]
      Endif

      usrdue = usrdue + xdue
      usrdsc = usrdsc + xdsc
      usrtax = usrtax + xtax
      usrcrdt = usrcrdt + xcrdt
      usrcash = usrcash + xcash

      With asx
        .Add("Not Grouped")
        .Add(modReportVar.GetLocaleNumberFormat(xdue, gb.Currency))
        .Add(modReportVar.GetLocaleNumberFormat(xdsc, gb.Currency))
        .Add(modReportVar.GetLocaleNumberFormat(xtax, gb.Currency))
        .Add(modReportVar.GetLocaleNumberFormat(xcrdt, gb.Currency))
        .Add(modReportVar.GetLocaleNumberFormat(xcash, gb.Currency))
      End With
      $BillingReport.Add(asx)
      asx.Clear()
    Endif

    With asx
      .Add("SUBTOTAL")
      .Add(modReportVar.GetLocaleNumberFormat(usrdue, gb.Currency))
      .Add(modReportVar.GetLocaleNumberFormat(usrdsc, gb.Currency))
      .Add(modReportVar.GetLocaleNumberFormat(usrtax, gb.Currency))
      .Add(modReportVar.GetLocaleNumberFormat(usrcrdt, gb.Currency))
      .Add(modReportVar.GetLocaleNumberFormat(usrcash, gb.Currency))
    End With
    $BillingReport.Add(asx)
    asx.Clear()

    totclear = 0
    totrnd = 0
    totcrnd = 0
    For Each xcomp In aList
      If sFormat = "Date" Then
        totclear = totclear + modFinance.GetInvoiceClearanceRange($con, bilid1, bilid2, "%", xcomp, sLocaType, sLocation, $tblpatbilldetail)
        totrnd = totrnd + modFinance.GetInvoiceCashRound($con, bilid1, bilid2, "%", xcomp, sLocaType, sLocation, $tblpatbilldetail)
        ''credit
        totcrnd = totcrnd + modFinance.GetInvoiceCreditRound($con, bilid1, bilid2, "%", xcomp, sLocaType, sLocation, $tblpatbilldetail, $tblpatbilling)
      Else
        totclear = totclear + modIDFinance.GetIDInvoiceClearanceRange($con, bilid1, bilid2, "%", xcomp, sLocaType, sLocation, $tblpatbilldetail)
        totrnd = totrnd + modIDFinance.GetIDInvoiceCashRound($con, bilid1, bilid2, "%", xcomp, sLocaType, sLocation, $tblpatbilldetail)
        ''credit
        totcrnd = totcrnd + modIDFinance.GetIDInvoiceCreditRound($con, bilid1, bilid2, "%", xcomp, sLocaType, sLocation, $tblpatbilldetail, $tblpatbilling)
      Endif
    Next

    With asx
      .Add(modString.HTMLBlankSpace(5) & "DUE CLEARANCES")
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add(modReportVar.GetLocaleNumberFormat(totclear, gb.Currency))
    End With
    $BillingReport.Add(asx)
    asx.Clear()

    With asx
      .Add(modString.HTMLBlankSpace(5) & "CASH ROUNDING")
      .Add("")
      .Add("")
      .Add("")
      .Add("")
      .Add(modReportVar.GetLocaleNumberFormat(totrnd, gb.Currency))
    End With
    $BillingReport.Add(asx)
    asx.Clear()

    With asx
      .Add(modString.HTMLBlankSpace(5) & "CASH FROM CREDIT ROUNDING")
      .Add("")
      .Add("")
      .Add("")
      .Add(modReportVar.GetLocaleNumberFormat(0 - totcrnd, gb.Currency))
      .Add(modReportVar.GetLocaleNumberFormat(totcrnd, gb.Currency))
    End With
    $BillingReport.Add(asx)
    asx.Clear()

    With asx
      .Add("<b>GRAND TOTAL</b>")
      .Add("")
      .Add("")
      .Add("")
      .Add(modReportVar.GetLocaleNumberFormat(usrcrdt - totcrnd, gb.Currency))
      .Add(modReportVar.GetLocaleNumberFormat(usrcash + totclear + totrnd + totcrnd, gb.Currency))
    End With
    $BillingReport.Add(asx)
    asx.Clear()

    Return RepoVal
  Endif

End

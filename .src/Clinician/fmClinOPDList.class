Public Sub btnotice_Click()

  Dim xData As String
  Dim xlst As String[]
  Dim Row As Integer

  xlst = New String[]
  If GridView1.Selection.Count Then
    Row = GridView1.Selection[0]
    $rData.MoveTo(GridView1.Selection[0])
    xData = InputCombo("Select Notice", $rData["fldencounterval"], xlst, "", False)
    If xData Then
      If $sTable = "tblconsult" Then
        modPatientSub.UpdateConsultNotice($rData["fldid"], xData)
      Else If $sTable = "tblopvisit" Then
        modPatientSub.UpdateOPVisitNotice($rData["fldid"], xData)
      Endif
    Endif
    FillBedGrid()
    GridView1.Select(Row)
  Endif

End

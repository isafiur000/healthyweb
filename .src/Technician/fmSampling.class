  Endif
  res = modDatabase.$myConn.Exec(sql, "Diagnostic Tests", "Waiting", True, modBasic.$compID, "%", xcomp, cmbcategory.Text)

  If res.Available Then
    For Each res
      With asx
        .Add(res!fldencounterval)
        .Add(modPatient.GetPatientNameByEnc(res!fldencounterval))

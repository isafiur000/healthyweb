' Gambas class file

Private $FmTxtVal As WebValueBox

Public Sub WebForm_Open()

  txtvalue.Text = ""

End

Public Sub SetValueBox(sValBox As WebValueBox)

  $FmTxtVal = sValBox
  txtvalue.Text = ""

End

Public Sub buttons_Click()

  If $FmTxtVal Then
    txtvalue.Text = txtvalue.Text & Last.Text
  Endif

End

Public Sub WebButton12_Click()

  txtvalue.Text = ""

End

Public Sub btnsave_Click()

  If Len(Trim(txtvalue.Text)) Then
    $FmTxtVal.Value = CFloat(txtvalue.Text)
  Endif

End

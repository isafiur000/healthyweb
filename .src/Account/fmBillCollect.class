  Dim res As Result

  If GridView1.Selection.Count Then
    $rData.MoveTo(GridView1.Selection[0])
    If rbwaiting.Value = True Then
      If Message.Question(("Are you sure ?"), ("No"), ("Yes")) = 2 Then
        res = modDatabase.$myConn.Edit("tblusercollection", "fldid=&1 and fldsave=&2", $rData["fldid"], False)
        res["flduserid_verify"] = modBasic.$lbluser
        res["fldtime_verify"] = Now()
        res["fldcomp_verify"] = modBasic.$compID
        res["fldstatus"] = "Verified"
        res["fldsave"] = True
        res.Update
        Me.Exec(Subst("Toastify({text: '&1', duration: &2}).showToast()", "Information updated", modBasic.$BalloonDelay))
        ShowCOllection()
      Endif
    Endif
  Endif

End

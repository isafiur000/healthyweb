  If Not $TestList.Count Then
    If sDepart = "All Sections" Then
      sstr1 = "select distinct(fldgroupname) as col from tblgroupradio where fldgroupname in(select flditemname from tblservicecost where (fldgroup=&1 or fldgroup=&2) and fldstatus=&3) ORDER BY fldgroupname"                                                ''
      res = modDatabase.$myConn.Exec(sstr1, $BillMode, "%", "Active")
    Else

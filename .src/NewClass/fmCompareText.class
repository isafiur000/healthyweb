' Gambas class file

Private $sTitle As String
Private $sInput As String
Private $sValue As Boolean

Public Sub Run(sInput As String, sTitle As String) As Boolean

  $sInput = sInput
  $sTitle = sTitle

  WebLabel10.Text = $sTitle
  $sValue = False

  If Me.ShowModal() Then Return $sValue

End

Public Sub WebButton1_Click()

  If WebTextBox1.Text = $sInput Then
    $sValue = True
    Me.Close(True)
  Else
    WebLabel1.Text = "Incorrect Code"
  Endif

End

Public Sub WebButton2_Click()

  Me.Close()

End

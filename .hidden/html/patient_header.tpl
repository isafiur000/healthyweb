<table class='header_table' style="text-align: left; width: 100%;" border="0" cellpadding="2" cellspacing="0" id="headtable">
  <tbody>
    
    <tr>
    
    <td style="vertical-align: top;font-weight:bold;width:10%">Name </td>
    <td style="vertical-align: top;width:35%">: {PatientName}</td>
      
    <td style="vertical-align: top;font-weight:bold;width:10%">EncID </td>
    <td style="vertical-align: top;width:25%">: {Encounter}</td>
      
    <td colspan="1" rowspan="4" style="vertical-align: top; width: 20%; height: auto; text-align: center;"><img style="width: 50%; height: auto%;" alt="" src={barcodelink}></td>
    
   </tr>
   
   
   <tr>
    
    <td style="vertical-align: top;font-weight:bold;width:10%">Age/Sex </td>
    <td style="vertical-align: top;width:35%">: {Age/Sex}</td>
      
    <td style="vertical-align: top;font-weight:bold;width:10%">DOReg </td>
    <td style="vertical-align: top;width:25%">: {RegistrationDate}</td>
      
    </tr>
    
    
   <tr>
       
    <td style="vertical-align: top;font-weight:bold;width:10%">Address </td>
    <td style="vertical-align: top;width:35%">: {PatientAddress}</td>
      
    <td style="vertical-align: top;font-weight:bold;width:10%">Phone </td>
    <td style="vertical-align: top;width:25%">: {ContactNo}</td>
      
    </tr>
    
    <tr>
    
    <td style="vertical-align: top;font-weight:bold;width:10%">PAN/NHIS </td>
    <td style="vertical-align: top;width:35%">: {PatientCode}</td>
      
    <td style="vertical-align: top;font-weight:bold;width:10%">ClaimCode </td>
    <td style="vertical-align: top;width:25%">: {LastInvReference}</td>
      
    </tr>
    
    <tr>
    
    <td style="vertical-align: top;font-weight:bold;width:10%">{Report} </td>
    <td style="vertical-align: top;width:35%">: {PARAM1}</td>
   
    </tr>
    
  </tbody>
</table>


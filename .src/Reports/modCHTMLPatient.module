Public Function ShowPatientDiagnosticImage(sType As String, fldid As Long, encid As String) As String

  Dim $BillingReport As CReportHTML
  Dim res As Result
  Dim asx As New String[0]
  Dim tempFile As String
  Dim hFile As Blob
  Dim aFile As String
  Dim Row As Integer

  $BillingReport = New CReportHTML([("SNO"), ("Description"), ("Image")], "PatientReport", encid)
  $BillingReport.UserData.Add("REPORT", "Report")
  If sType = "Laboratory" Then
    $BillingReport.UserData.Add(modLabTest.GetTestnameFromTestID(fldid), "PARAM1")
  Else
    $BillingReport.UserData.Add("", "PARAM1")
  Endif

  Row = 1
  res = modDatabase.$syConn.Exec("select fldtime,fldpic,fldtitle,flddetail,fldlink from tblpatserialimage where fldtestid=&1 and fldencounterval=&2 and fldcateg=&3 and fldmode=&4", fldid, encid, "IMAGE", sType)
  If res.Available Then
    For Each res
      tempFile = Temp() & ".jpg"
      If res["fldlink"] Then
        aFile = modFTPSub.GetFileFromLocalFTP(res["fldlink"])
        If Exist(aFile) Then
          Copy aFile To tempFile
        Endif
      Else
        hFile = res["fldpic"]
        If hFile.Length Then
          File.Save(tempFile, hFile.Data)
        Endif
      Endif

      With asx
        .Add(CStr(Row))
        .Add(modString.TextToHTML(res["fldtitle"] & gb.NewLine & res["flddetail"] & "[" & modReportVar.GetDateTimeReport(res["fldtime"], gb.GeneralDate) & "]"))
        .Add(modString.GetImageForHTML(tempFile))
      End With
      $BillingReport.Add(asx)
      asx.Clear()
    Next
  Endif

  Return $BillingReport.NewHTMLPath()

End


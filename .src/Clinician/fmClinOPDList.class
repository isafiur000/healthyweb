Private Function GetConsultPrecomment(sIndex As Long) As String

  Dim res As Result
  Dim xx As String

  res = modDatabase.$myConn.Exec(Subst("select fldpresummary from &1", $sTable) & " where fldid=&1", sIndex)
  If res.Available Then
    xx = res["fldpresummary"]
  Else
    xx = ""
  Endif
  Return xx

End


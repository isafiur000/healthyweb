Public Function GetServiceSectionArray(sType As String, sBillMode As String) As String[]

  Dim sql As String
  Dim res As Result
  Dim xval As String[]

  xval = New String[]
  sql = "select flditemname,fldreport from tblservicecost where flditemtype=&1 and (fldgroup like &2 or fldgroup=&4) and fldstatus=&3 ORDER BY flditemname"
  res = modDatabase.$myConn.Exec(sql, sType, sBillMode, "Active", "%")
  If res.Available Then
    For Each res
      If res["fldreport"] Then
        xval.Add(res["fldreport"] & "|" & res["flditemname"])
      Else
        xval.Add("Unspecified" & "|" & res["flditemname"])
      Endif
    Next
  Endif

  Return xval

End


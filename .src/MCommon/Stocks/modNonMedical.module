Public Function GetBillIDFromBillNo(billno As String, Optional sTable As String, Optional sCon As Connection) As Long

  Dim res As Result
  Dim xx As Long
  Dim $con As Connection

  If sCon Then
    $con = sCon
  Else
    $con = modDatabase.$myConn
  Endif

  If billno Then
    If sTable Then
      GetInvoiceDataColl(billno, sTable, $con)
    Else
      GetInvoiceDataColl(billno, "tblpatbilldetail", $con)
    Endif

    If $InvoiceTableData.Exist(billno) Then
      xx = $InvoiceTableData[billno]["fldid"]
    Else
      If sTable Then
        res = $con.Exec("select fldid from " & sTable & " where fldbillno=&1", billno)
      Else
        res = $con.Exec("select fldid from tblpatbilldetail where fldbillno=&1", billno)
      Endif
      If res.Available Then
        xx = res["fldid"]
      Else
        xx = 0
        Message.Optional(billno & (" not correct"), ("OK"), ("Disable Warning"))
      Endif
    Endif

  Else
    xx = 0
  Endif
  Return xx

End


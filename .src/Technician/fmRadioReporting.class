Private Sub FillSamplingEncounters()

  Dim sql As String
  Dim xdate As String
  Dim xcateg As String
  Dim xsample As String
  Dim xsearch As String
  Dim xpreun As String
  Dim xunion As String
  Dim xorder As String

  If chkdate.Value = True Then
    xdate = " and tblpatradiotest.fldnewdate>=&4 and tblpatradiotest.fldnewdate<=&5"
  Else
    If modBasic.$SQLDateRange = "Disable" Then
      xdate = ""
    Else
      xdate = db.Subst(" and tblpatradiotest.fldnewdate>=&1 and tblpatradiotest.fldnewdate<=&2", modDate.StartSqlDate(DateAdd(dtsort.Value, gb.Day, 0 - 365)), modDate.EndSqlDate(dtsort.Value))
    Endif
  Endif

  If cmbcategory.Text = "%" Then
    xcateg = ""
  Else
    xcateg = " and tblpatradiotest.fldtestid in(select fldexamid from tblradio where fldcategory like &6)"
  Endif

  If cmbrepocounter.Text Then
    xsample = " and tblpatradiotest.fldbcounter like &7"
  Else
    xsample = ""
  Endif

  If txtsearch.Text Then
    If rbencrepo.Value = True Then
      xsearch = " and lower(tblpatradiotest.fldencounterval) like &8"
    Else If rbinvoicrepo.Value = True Then
      xsearch = " and lower(tblpatradiotest.fldbillno) like &8"
    Else If rbindoorrepo.Value = True Then
      xsearch = " and lower(tblpatradiotest.fldencounterval) like &8"
    Endif
  Else
    xsearch = ""
  Endif

  If rbdate.Value = True Then
    xpreun = ""
    xunion = ""
    xorder = " ORDER BY tblpatradiotest.fldnewdate"
  Else If rbtriage.Value = True Then
    xpreun = ""
    xunion = " inner join tblencounter on tblpatradiotest.fldencounterval=tblencounter.fldencounterval"
    xorder = " ORDER BY FIELD(tblencounter.fldheight, '0', '65280', '16776960', '16711680') DESC"
  Else If rbname.Value = True Then
    xpreun = "("
    xunion = " inner join tblencounter on tblpatradiotest.fldencounterval=tblencounter.fldencounterval) inner join tblpatientinfo on tblencounter.fldpatientval=tblpatientinfo.fldpatientval"
    xorder = " ORDER BY tblpatientinfo.fldptnamefir"
  Endif

  If rbencrepo.Value = True Then
    sql = "select tblpatradiotest.fldencounterval as fldparent,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval,tblpatradiotest.fldtestid,tblpatradiotest.fldencounterval,tblpatradiotest.fldid,tblpatradiotest.fldnewdate,tblpatradiotest.fldsampletype,tblpatradiotest.fldabnormal,tblpatradiotest.fldbcounter,tblpatradiotest.fldnewdate,tblpatradiotest.fldmethod,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval from " & xpreun & "tblpatradiotest" & xunion & " where tblpatradiotest.fldstatus=&1 and tblpatradiotest.fldsave_report=&2 and tblpatradiotest.fldcomp_report=&3" & xdate & xcateg & xsample & xsearch & xorder                                        ''
  Else If rbinvoicrepo.Value = True Then
    sql = "select tblpatradiotest.fldbillno as fldparent,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval,tblpatradiotest.fldtestid,tblpatradiotest.fldencounterval,tblpatradiotest.fldid,tblpatradiotest.fldnewdate,tblpatradiotest.fldsampletype,tblpatradiotest.fldabnormal,tblpatradiotest.fldbcounter,tblpatradiotest.fldnewdate,tblpatradiotest.fldmethod,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval from " & xpreun & "tblpatradiotest" & xunion & " where tblpatradiotest.fldstatus=&1 and tblpatradiotest.fldsave_report=&2 and tblpatradiotest.fldcomp_report=&3" & xdate & xcateg & xsample & xsearch & xorder
  Else If rbindoorrepo.Value = True Then
    sql = "select tblpatradiotest.fldencounterval as fldparent,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval,tblpatradiotest.fldtestid,tblpatradiotest.fldencounterval,tblpatradiotest.fldid,tblpatradiotest.fldnewdate,tblpatradiotest.fldsampletype,tblpatradiotest.fldabnormal,tblpatradiotest.fldbcounter,tblpatradiotest.fldnewdate,tblpatradiotest.fldmethod,tblpatradiotest.fldencounterval,tblpatradiotest.fldencounterval from " & xpreun & "tblpatradiotest" & xunion & " where tblpatradiotest.fldstatus=&1 and tblpatradiotest.fldsave_report=&2 and tblpatradiotest.fldcomp_report=&3" & xdate & xcateg & xsample & xsearch & xorder                                      ''
  Endif
  If rbpending.Value = True Then
    $rData2 = modDatabase.$myConn.Exec(sql, "Sampled", False, modBasic.$compID, modDate.StartSqlDate(dtsort.value), modDate.EndSqlDate(DateAdd(dtsort.Value, gb.Day, txtrangerepo.Value)), cmbcategory.Text, cmbrepocounter.Text & "%", LCase(Trim(txtsearch.Text)))
  Else If rbscanned.Value = True Then
    $rData2 = modDatabase.$myConn.Exec(sql, "Scanned", False, modBasic.$compID, modDate.StartSqlDate(dtsort.value), modDate.EndSqlDate(DateAdd(dtsort.Value, gb.Day, txtrangerepo.Value)), cmbcategory.Text, cmbrepocounter.Text & "%", LCase(Trim(txtsearch.Text)))
  Else If rbreported.Value = True Then
    $rData2 = modDatabase.$myConn.Exec(sql, "Reported", True, modBasic.$compID, modDate.StartSqlDate(dtsort.value), modDate.EndSqlDate(DateAdd(dtsort.Value, gb.Day, txtrangerepo.Value)), cmbcategory.Text, cmbrepocounter.Text & "%", LCase(Trim(txtsearch.Text)))
  Endif
  $aMyFields2 = New String[]
  modGridView.ReadSmallData(GridView2, $rData2, $aMyFields2)
  With GridView2
    If rbinvoice.Value = True Then
      .Columns[0].Width = CStr(150 * modBasic.$AppWidthRatio) & "px"
      .Columns[0].Text = "Invoice"
    Else
      .Columns[0].Width = CStr(125 * modBasic.$AppWidthRatio) & "px"

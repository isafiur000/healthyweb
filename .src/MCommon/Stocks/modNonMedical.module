End

''Get Category from route combo (used in tblpatbilling)
Public Sub GetBillItemCategoryFromCombo(txtitem As String) As String

  Dim txt As String

  If txtitem = "Test" Then
    txt = "Diagnostic Tests"
  Else If txtitem = "Radio" Then
    txt = "Radio Diagnostics"
  Else If txtitem = "Service" Then
    txt = "General Services"
  Else If txtitem = "Procedure" Then
    txt = "Procedures"
  Else If txtitem = "Equipment" Then
    txt = "Equipment"
  Else If txtitem = "Others" Then
    txt = "Other Items"

  Else If txtitem = "oral" Or txtitem = "liquid" Or txtitem = "fluid" Or txtitem = "injection" Or txtitem = "resp" Or txtitem = "topical" Or txtitem = "eye/ear" Or txtitem = "anal/vaginal" Or txtitem = "IVpush" Or txtitem = "CIV" Or txtitem = "IIV" Or txtitem = "SC" Or txtitem = "IM" Or txtitem = "IT" Or txtitem = "IDer" Or txtitem = "ICar" Or txtitem = "ISyn" Then                                                   '''''
    txt = "Medicines"
  Else If txtitem = "suture" Or txtitem = "msurg" Or txtitem = "ortho" Then
    txt = "Surgicals"
  Else If txtitem = "extra" Then
    txt = "Extra Items"
  Endif

  Return txt

End

Public Function GetServiceKeyFromCategory(txttype As String) As String

  Dim txtitem As String

  If txttype = "Diagnostic Tests" Then
    txtitem = "Test"
  Else If txttype = "Radio Diagnostics" Then
    txtitem = "Radio"
  Else If txttype = "General Services" Then
    txtitem = "Service"
  Else If txttype = "Procedures" Then
    txtitem = "Procedure"
  Else If txttype = "Equipment" Then
    txtitem = "Equipment"
  Else If txttype = "Other Items" Then
    txtitem = "Others"
  Else
    txtitem = ""
  Endif
  Return txtitem

End

Public Function GetServiceComboFromCategory(sItem As String) As String

  Dim txtitem As String
  Dim txttype As String

  txttype = GetBillingCategory(sItem)
  If txttype Then
    txtitem = GetServiceKeyFromCategory(txttype)
  Else
    txtitem = ""
  Endif

  Return txtitem

End

Public Function CashCreditRow(Optional sTable As String) As String

  Dim xx As String
  Dim xtable As String

  If sTable Then
    xtable = sTable
  Else
    xtable = "tblpatbilling"
  Endif

  xx = Subst("((REPLACE(REPLACE(&1.fldbilltype , 'Cash', 0), 'Credit', 100) - COALESCE(&1.fldcashincredit, 0)) * &1.fldditemamt / 100) as xcredit,((REPLACE(REPLACE(&1.fldbilltype , 'Cash', 100), 'Credit', 0) + COALESCE(&1.fldcashincredit, 0)) * &1.fldditemamt / 100) as xcash", xtable)
  Return xx

End

Public Function CashCreditSQL(Optional sTable As String) As String

  Dim xx As String
  Dim xtable As String

  If sTable Then
    xtable = sTable
  Else
    xtable = "tblpatbilling"
  Endif

  xx = Subst("SUM((REPLACE(REPLACE(&1.fldbilltype , 'Cash', 0), 'Credit', 100) - COALESCE(&1.fldcashincredit, 0)) * &1.fldditemamt / 100) as xcredit,SUM((REPLACE(REPLACE(&1.fldbilltype , 'Cash', 100), 'Credit', 0) + COALESCE(&1.fldcashincredit, 0)) * &1.fldditemamt / 100) as xcash", xtable)
  Return xx

End

Public Function CashCreditBillSQL(Optional sTable As String) As String

  Dim xx As String
  Dim xtable As String

  If sTable Then
    xtable = sTable
  Else
    xtable = "tblpatbilling"
  Endif

  xx = Subst("SUM(&1.flditemrate*&1.flditemqty) as duemt,SUM(&1.flddiscamt) as disctot,SUM(&1.fldtaxamt) as taxtot,SUM(&1.fldditemamt) as netamt,SUM((REPLACE(REPLACE(&1.fldbilltype , 'Cash', 0), 'Credit', 100) - COALESCE(&1.fldcashincredit, 0)) * &1.fldditemamt / 100) as xcredit,SUM((REPLACE(REPLACE(&1.fldbilltype , 'Cash', 100), 'Credit', 0) + COALESCE(&1.fldcashincredit, 0)) * &1.fldditemamt / 100) as xcash", xtable)
  Return xx

End

Public Sub GetInvoiceDataColl(sBillNo As String, sTable As String, sCon As Connection)

  Dim res As Result
  Dim sColl As Collection

  If $InvoiceTableData.Exist(sBillNo) = False Then
    res = sCon.Exec("SELECT fldid,fldencounterval,flditemamt,fldtaxamt,flddiscountamt,fldchargedamt,fldreceivedamt,fldcurdeposit,fldbilltype,fldchequeno,fldbankname,fldtaxgroup,flddiscountgroup,fldprevdeposit,flduserid,fldtime,fldcomp,fldhostmac FROM " & sTable & " WHERE fldbillno=&1", sBillNo)
    If res.Available Then
      sColl = New Collection
      sColl.Add(res["fldid"], "fldid")
      sColl.Add(res["fldencounterval"], "fldencounterval")

      If res["flditemamt"] Then
        sColl.Add(res["flditemamt"], "flditemamt")
      Else
        sColl.Add(0, "flditemamt")
      Endif
      If res["fldtaxamt"] Then
        sColl.Add(res["fldtaxamt"], "fldtaxamt")
      Else
        sColl.Add(0, "fldtaxamt")
      Endif
      If res["flddiscountamt"] Then
        sColl.Add(res["flddiscountamt"], "flddiscountamt")
      Else
        sColl.Add(0, "flddiscountamt")
      Endif
      If res["fldchargedamt"] Then
        sColl.Add(res["fldchargedamt"], "fldchargedamt")
      Else
        sColl.Add(0, "fldchargedamt")
      Endif
      If res["fldreceivedamt"] Then
        sColl.Add(res["fldreceivedamt"], "fldreceivedamt")
      Else
        sColl.Add(0, "fldreceivedamt")
      Endif
      If res["fldcurdeposit"] Then
        sColl.Add(res["fldcurdeposit"], "fldcurdeposit")
      Else
        sColl.Add(0, "fldcurdeposit")
      Endif

      sColl.Add(res["fldbilltype"], "fldbilltype")
      sColl.Add(res["fldchequeno"], "fldchequeno")
      sColl.Add(res["fldbankname"], "fldbankname")

      If res["fldprevdeposit"] Then
        sColl.Add(res["fldprevdeposit"], "fldprevdeposit")
      Else
        sColl.Add(0, "fldprevdeposit")
      Endif

      sColl.Add(res["fldtaxgroup"], "fldtaxgroup")
      sColl.Add(res["flddiscountgroup"], "flddiscountgroup")
      sColl.Add(res["flduserid"], "flduserid")
      sColl.Add(res["fldtime"], "fldtime")
      sColl.Add(res["fldcomp"], "fldcomp")
      sColl.Add(res["fldhostmac"], "fldhostmac")

      ''add to collection
      $InvoiceTableData.Add(sColl, sBillNo)
    Endif
  Endif

End


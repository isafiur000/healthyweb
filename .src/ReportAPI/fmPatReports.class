          Endif
        Else
          If MMain.$WebEntry = True Then
            modDatabase.$myConn.Delete($tblpatreport, "fldrepoid=&1 and fldsave=&2", $rData["fldrepoid"], True)
          Else
            modDatabase.$myConn.Delete($tblpatreport, "fldid=&1 and fldsave=&2", $rData["fldid"], True)
          Endif
          FillGridData()
        Endif

      Endif
    Endif

Public Function BillItemCategory() As String[]

  Dim xxx As String[]

  xxx = New String[]
  xxx.Add("Medicines")
  xxx.Add("Surgicals")
  xxx.Add("Extra Items")

  xxx.Add("Diagnostic Tests")
  xxx.Add("Radio Diagnostics")
  xxx.Add("General Services")
  xxx.Add("Procedures")
  xxx.Add("Equipment")
  xxx.Add("Other Items")
  xxx.Sort(gb.Ascent)

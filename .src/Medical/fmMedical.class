Public Sub mnuchecklist_Click()

  Dim hForm As FmAddVarTwo
  Dim xList As String[]
  Dim yList As String[]

  xList = modControlSub.GetDirectFillresult(modDatabase.$myConn.Exec("select distinct(fldtitle) as col from tblchecklist"))
  yList = New String[]
  hForm = New FmAddVarTwo("fldtitle", "fldquery", "tblchecklist", xList, yList)
  hForm.ShowModal

End


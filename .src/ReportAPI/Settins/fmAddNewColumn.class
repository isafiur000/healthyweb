Private Sub ShowFilecustom(sInt As Integer)

  If sInt = 0 Then
    ShowCustColumn(TabPanel1, cmbvariable1, txtsql1, txtfiroption1, txtsecoption1, txtthirdoption1, txttitle1, chkarry1, txtscript1, txtshell1, cmbformat1, chktrans1)
  Else If sInt = 1 Then
    ShowCustColumn(TabPanel1, cmbvariable2, txtsql2, txtfiroption2, txtsecoption2, txtthirdoption2, txttitle2, chkarry2, txtscript2, txtshell2, cmbformat2, chktrans2)
  Else If sInt = 2 Then
    ShowCustColumn(TabPanel1, cmbvariable3, txtsql3, txtfiroption3, txtsecoption3, txtthirdoption3, txttitle3, chkarry3, txtscript3, txtshell3, cmbformat3, chktrans3)
  Else If sInt = 3 Then
    ShowCustColumn(TabPanel1, cmbvariable4, txtsql4, txtfiroption4, txtsecoption4, txtthirdoption4, txttitle4, chkarry4, txtscript4, txtshell4, cmbformat4, chktrans4)
  Else If sInt = 4 Then
    ShowCustColumn(TabPanel1, cmbvariable5, txtsql5, txtfiroption5, txtsecoption5, txtthirdoption5, txttitle5, chkarry5, txtscript5, txtshell5, cmbformat5, chktrans5)
  Else If sInt = 5 Then
    ShowCustColumn(TabPanel1, cmbvariable6, txtsql6, txtfiroption6, txtsecoption6, txtthirdoption6, txttitle6, chkarry6, txtscript6, txtshell6, cmbformat6, chktrans6)
  Else If sInt = 6 Then
    ShowCustColumn(TabPanel1, cmbvariable7, txtsql7, txtfiroption7, txtsecoption7, txtthirdoption7, txttitle7, chkarry7, txtscript7, txtshell7, cmbformat7, chktrans7)
  Else If sInt = 7 Then
    ShowCustColumn(TabPanel1, cmbvariable8, txtsql8, txtfiroption8, txtsecoption8, txtthirdoption8, txttitle8, chkarry8, txtscript8, txtshell8, cmbformat8, chktrans8)
  Else If sInt = 8 Then
    ShowCustColumn(TabPanel1, cmbvariable9, txtsql9, txtfiroption9, txtsecoption9, txtthirdoption9, txttitle9, chkarry9, txtscript9, txtshell9, cmbformat9, chktrans9)
  Else If sInt = 9 Then
    ShowCustColumn(TabPanel1, cmbvariable10, txtsql10, txtfiroption10, txtsecoption10, txtthirdoption10, txttitle10, chkarry10, txtscript10, txtshell10, cmbformat10, chktrans10)
  Endif

End


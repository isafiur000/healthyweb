' Gambas class file

Static Public Sub _call(sType As String) As String

  Dim xx As String
  Dim encid As String

  If sType = "Encounter" Then
    xx = InputBox("Patient Number To EncounterID Conversion", "Search Patient", "")
    If xx Then
      encid = modPatient.GetLastEncoutnerFromPatNo(Trim(xx))
      Return encid
    Endif
  Else
    Return fmPatSearch.Run(sType)
  Endif

End

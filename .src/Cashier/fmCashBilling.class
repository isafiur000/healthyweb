Private Sub CopyToNewItem(encidLast As String, xItem As String)

  Dim res As Result
  Dim res1 As Result

  modDatabase.$myConn.Begin
  res = modDatabase.$myConn.Edit("tblpatbilling", "fldencounterval=&1 and fldsave=&2 and fldstatus=&3 and flditemname=&4 and fldbillno IS NULL", encidLast, False, "Planned", xItem)
  If res.Available Then
    res.MoveFirst

    If res["flditemtype"] = "Radio Diagnostics" Then
      res1 = modDatabase.$myConn.Edit("tblpatradiotest", "fldencounterval=&1 and fldgroupid=&2", encidLast, res["fldid"])
      If res1.Available Then
        res1["fldencounterval"] = Trim(txtencid.Text)
        res1.Update
      Endif
    Else If res["flditemtype"] = "Procedures" Then
      res1 = modDatabase.$myConn.Edit("tblpatgeneral", "fldencounterval=&1 and fldgroupid=&2 and flditem=&3", encidLast, res["fldid"], res["flditemname"])
      If res1.Available Then
        res1["fldencounterval"] = Trim(txtencid.Text)
        res1.Update
      Endif
    Endif

    res["fldencounterval"] = Trim(txtencid.Text)
    res["fldstatus"] = "Punched"
    res.Update
  Endif
  modDatabase.$myConn.Commit

Catch
  modDatabase.$myConn.Rollback
  modHelpVariable.CreateErrorReport()

End


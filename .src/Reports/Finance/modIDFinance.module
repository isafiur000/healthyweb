Public Function GetIDInvoiceCreditRound($conn As Connection, bilid1 As Long, bilid2 As Long, sUser As String, sComp As String, sLocaType As String, sLocation As String, Optional sBillDetail As String, Optional sBillTable As String) As Float

  Dim res As Result
  Dim xval As Float
  Dim res1 As Result
  Dim yval As Float
  Dim xdiff As Float

  Dim RepoStr As String
  Dim RepoStr1 As String
  Dim xbilldetail As String
  Dim xbilling As String

  If sBillDetail Then
    xbilldetail = sBillDetail
  Else
    xbilldetail = "tblpatbilldetail"
  Endif
  If sBillTable Then
    xbilling = sBillTable
  Else
    xbilling = "tblpatbilling"
  Endif

  RepoStr = modDataRepo.GetWhereStringRepo(sLocaType, sLocation)
  res = $conn.Exec("select SUM(fldreceivedamt) as totamt from " & xbilldetail & " where fldid>=&1 and fldid<=&2 and fldbilltype=&3 and flduserid like &4 and fldcomp like &5 and flditemamt<>&6" & RepoStr, bilid1, bilid2, "Credit", sUser, sComp, 0)                                        ''
  If res.Available Then
    If res["totamt"] Then
      xval = res["totamt"]
    Else
      xval = 0
    Endif
  Else
    xval = 0
  Endif

  RepoStr1 = modDataRepo.GetWhereStringRepo(sLocaType, sLocation, "t1")
  res1 = $conn.Exec("select SUM((REPLACE(REPLACE(t1.fldbilltype , 'Cash', 100), 'Credit', 0) + COALESCE(t1.fldcashincredit, 0)) * t1.fldditemamt / 100) as xcash from " & xbilling & " AS t1 inner join " & xbilldetail & " AS t2 ON t1.fldbillno=t2.fldbillno where t2.fldid>=&1 and t2.fldid<=&2 and t2.fldbilltype=&3 and t2.flduserid like &4 and t2.fldcomp like &5 and t2.flditemamt<>&6" & RepoStr1, bilid1, bilid2, "Credit", sUser, sComp, 0)                                                                    ''
  If res1.Available Then
    If res1["xcash"] Then
      yval = res1["xcash"]
    Else
      yval = 0
    Endif
  Else
    yval = 0
  Endif

  xdiff = xval - yval

  Return xdiff

End


                res["fldditemamt"] = res["flditemrate"] * res["flditemqty"] * (1 - xval / 100) * (1 + res["fldtaxper"] / 100)
                res["xyz"] = False
                res.Update
                ShowNewEntry()
              Endif
            Endif
          Endif

        Else
          Message.Warning(("Discount alteration not allowed"), ("OK"))
        Endif
      Endif
    Endif
  Endif


Public Function GetBillTypeFromInvoice(billno As String, Optional sCon As Connection) As String

  Dim res As Result
  Dim res1 As Result
  Dim xval As String
  Dim xtype As String
  Dim $con As Connection

  If sCon Then
    $con = sCon
  Else
    $con = modDatabase.$myConn
  Endif

  xtype = modBillLock.GetInvoiceFormatNo(billno)
  If xtype = "Invoice" Then
    GetInvoiceDataColl(billno, "tblpatbilldetail", $con)
    If $InvoiceTableData.Exist(billno) Then
      xval = $InvoiceTableData[billno]["fldbilltype"]
    Else
      res = modDatabase.$myConn.Exec("select fldbilltype from tblpatbilldetail where fldbillno=&1", billno)
      If res.Available Then
        If res["fldbilltype"] Then
          xval = res["fldbilltype"]
        Else
          xval = ""
        Endif
      Else
        xval = ""
      Endif
    Endif

  Else If xtype = "Receipt" Then
    GetReceiptDataColl(billno, "tbltempbilldetail", $con)
    If $ReceiptTableData.Exist(billno) Then
      xval = $ReceiptTableData[billno]["fldbilltype"]
    Else
      res1 = modDatabase.$myConn.Exec("select fldbilltype from tbltempbilldetail where fldbillno=&1", billno)
      If res1.Available Then
        If res1["fldbilltype"] Then
          xval = res1["fldbilltype"]
        Else
          xval = ""
        Endif
      Else
        xval = ""
      Endif
    Endif

  Endif

  Return xval

End


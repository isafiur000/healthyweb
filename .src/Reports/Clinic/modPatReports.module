Public Function GetFormattedExaminationReport(encid As String, sExamList As String[]) As String

  Dim sExam As String
  Dim res As Result
  Dim xlabel As String
  Dim xtabval As String
  Dim xval As String

  Dim xFinal As Variant[]
  Dim i As Integer
  Dim xRowVal As Collection
  Dim colLst As String[]
  Dim xtable As String

  xFinal = New Variant[]
  i = 1

  If modBasic.$ShowDayMedReport = "User" Then
    colLst = ["", "Variable", "Value", "User"]
  Else If modBasic.$ShowDayMedReport = "Date+User" Then
    colLst = ["", "Date", "Variable", "Value", "User"]
  Else If modBasic.$ShowDayMedReport = "Date" Then
    colLst = ["", "Date", "Variable", "Value"]
  Else
    colLst = ["", "Variable", "Value"]
  Endif

  For Each sExam In sExamList
    res = modDatabase.$syConn.Exec("select fldid,fldtype,fldtime,fldinput,fldhead,fldoption,fldcomp,fldmethod,flduserid from tblpatientexam where fldencounterval=&1 and fldinput=&2 and fldhead=&3 and fldsave=&4", encid, "Physician Examinations", sExam, True)
    If res.Available Then
      res.MoveLast

      xRowVal = New Collection
      xRowVal.Add(CStr(i), CStr(0))

      If res["fldmethod"] = "Regular" Then
        xlabel = res["fldhead"]
      Else
        xlabel = res["fldhead"] & "<br>" & "<small>[" & res["fldmethod"] & "]</small>"
      Endif

      xval = ""
      If res["fldtype"] = "Quantitative" Then
        xval = modClinic.GetExamValueString(encid, res["fldid"], True)
      Else If res["fldtype"] = "Qualitative" Then
        xtabval = modClinic.GetSubExamQualiList(encid, res["fldoption"], res["fldid"])
        If xtabval Then
          xval = modString.TextToHTML(modClinic.GetExamValueString(encid, res["fldid"], True) & xtabval)
        Else
          xval = modClinic.GetExamValueString(encid, res["fldid"], True)
        Endif
      Endif

      If modBasic.$ShowDayMedReport = "User" Then
        xRowVal.Add(xlabel, CStr(1))
        xRowVal.Add(xval, CStr(2))
        xRowVal.Add(res["flduserid"], CStr(3))
      Else If modBasic.$ShowDayMedReport = "Date+User" Then
        xRowVal.Add(modReportVar.GetDateTimeReport(res["fldtime"], gb.MediumDate), CStr(1))
        xRowVal.Add(xlabel, CStr(2))
        xRowVal.Add(xval, CStr(3))
        xRowVal.Add(res["flduserid"], CStr(4))
      Else If modBasic.$ShowDayMedReport = "Date" Then
        xRowVal.Add(modReportVar.GetDateTimeReport(res["fldtime"], gb.MediumDate), CStr(1))
        xRowVal.Add(xlabel, CStr(2))
        xRowVal.Add(xval, CStr(3))
      Else
        xRowVal.Add(xlabel, CStr(1))
        xRowVal.Add(xval, CStr(2))
      Endif

      xFinal.Add(xRowVal)
      i = i + 1
    Endif
  Next

  xtable = modString.GetRichTableStringFromVariant(colLst, xFinal)

  Return xtable

End

''-------------------------Laboratory -------------------------------
Public Function GetlaboratoryHTMLProfile(encid As String, testList As String[], sUnit As String) As String

  Dim aList As String[]
  Dim categList As String[]
  Dim xcatList As String[]
  Dim xcateg As String

  Dim bList As String[]
  Dim xtest As String
  Dim asx As String[]

  Dim stObservation As String
  Dim xtable As String

  aList = New String[]
  categList = New String[]
  For Each xtest In testList
    xcateg = modFixLab.GetLabTestCategory(xtest)
    If Not xcateg Then
      xcateg = "Null"
    Endif
    categList.Add(xcateg)
    aList.Add(xcateg & "|" & xtest)
  Next
  xcatList = modString.BinaryDistinctStringArray(categList)

  stObservation = ""
  For Each xcateg In xcatList

    bList = New String[]
    For Each xtest In aList
      asx = Split(xtest, "|")
      If asx[0] = xcateg Then
        bList.Add(asx[1])
      Endif
    Next

    xtable = ""
    If modBasic.$ShowQuantiMedReport = "AllValues" Then
      xtable = GetCompleteTestByEnc(encid, bList, sUnit, False)
    Else If modBasic.$ShowQuantiMedReport = "Chart" Then
      xtable = GetCompleteTestByEnc(encid, bList, sUnit, True)
    Else
      xtable = GetLaboratoryHTMLTable(encid, bList, sUnit, modBasic.$ShowQualiMedReport)
    Endif
    If xcateg = "Null" Then
      stObservation = stObservation & "<b>" & ". . ." & "</b>" & gb.NewLine
    Else
      stObservation = stObservation & "<b>" & xcateg & "</b>" & gb.NewLine
    Endif
    stObservation = stObservation & xtable & "<br>"

  Next

  Return stObservation

End


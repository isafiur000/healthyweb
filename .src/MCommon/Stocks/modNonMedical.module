Public Function GetClaimWiseTotalAmt(sType As String, xClaim As String, xBankName As String, xBillMode As String) As Float

  Dim res As Result
  Dim xval As Float

  If sType = "Invoice" Then
    res = modDatabase.$myConn.Exec("select SUM(flditemamt+fldtaxamt-flddiscountamt-fldreceivedamt) as xtot from tblpatbilldetail where fldbankname=&1 and fldbilltype like &2 and fldchequeno=&3", xBankName, xBillMode, xClaim)
  Else If sType = "Receipt" Then
    res = modDatabase.$myConn.Exec("select SUM(flditemamt+fldtaxamt-flddiscountamt-fldreceivedamt) as xtot from tbltempbilldetail where fldbankname=&1 and fldbilltype like &2 and fldchequeno=&3", xBankName, xBillMode, xClaim)
  Endif
  If res.Available Then
    For Each res
      If res["xtot"] Then
        xval = xval + res["xtot"]
      Endif
    Next
  Endif
  Return xval

End

Public Function GetReportReference(encid As String, Optional xScheme As Boolean) As String

  Dim xdiscPack As String
  Dim resx As Result
  Dim xrefNo As String

  xdiscPack = modPatient.ShowDiscountCategEnc(encid)
  If xdiscPack Then
    resx = modDatabase.$myConn.Exec("select fldreference,fldacledger from tbldiscount where fldtype=&1", xdiscPack)
    If resx.Available Then

      Select resx["fldreference"]
        Case "Claim Code"
          xrefNo = modClaim.GetCurrentClaimCodePrint(encid)
        Case "Accident SSF", "Critical SSF", "MedExtra SSF", "Medical SSF"
          xrefNo = modSSF.GetSSFClaimVIewMedical(encid, resx["fldreference"])
      End Select

      If xrefNo Then
        If xScheme = True Then
          xrefNo = resx["fldacledger"] & "|" & xrefNo
        Endif
      Endif

    Endif
  Endif

  Return xrefNo

End


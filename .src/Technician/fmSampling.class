Private Sub GetNormalSampling()

  Dim Row As Integer
  Dim res As Result
  Dim xallow As Boolean
  Dim xspec As String
  Dim txtsampno As String
  Dim xorder As Integer
  Dim resx As Result

  If modBasic.$LabAutoSampleID = "Yes" Then
    If GridView1.Count Then
      If Not txtsamid.Text Then
        txtsamid.Text = modLabSub.GetAutoIncreasingNo()
      Endif
    Endif
  Endif

  If txtsamid.Text Then
    For Row = 0 To GridView1.Count - 1
      If GridView1.IsSelected(Row) = True Then
        $rData.MoveTo(Row)

        If modBasic.$LabAutoSampleSuffix = "Yes" Then
          txtsampno = Trim(txtsamid.Text) & modFixLab.GetTestSampleSuffix($rData["fldtestid"])
        Else
          txtsampno = Trim(txtsamid.Text)
        Endif
        xorder = modFixLab.GetLabTestOrder($rData["fldtestid"])

        xallow = modLabTest.AllowSampleID($encid, txtsampno)
        If xallow = True Then

          If modBasic.$LabAutoSpecimen = "Yes" Then
            xspec = modFixLab.GetTestSpecimen($rData["fldtestid"])
            If Not xspec Then
              xspec = Trim(cmbsamtype.Text)
            Endif
            modLabSub.UpdateLabTestSample($rData["fldid"], txtsampno, xspec, "", "", "", "", xorder,, $tblpatlabtest)
          Else
            If cmbsamtype.Text Then
              If dtcurr.Value Then
                modLabSub.UpdateLabTestSample($rData["fldid"], txtsampno, Trim(cmbsamtype.Text), "", "", "", "", xorder, dtcurr.Value, $tblpatlabtest)
              Else
                modLabSub.UpdateLabTestSample($rData["fldid"], txtsampno, Trim(cmbsamtype.Text), "", "", "", "", xorder,, $tblpatlabtest)
              Endif
            Endif
          Endif

          res = modDatabase.$myConn.Exec("select fldsubtest from " & $tblpatlabsubtest & " where fldtestid=&1", $rData["fldid"])
          If res.Available = False Then
            Select modFixLab.GetLabTestOptionType($rData["fldtestid"])
              Case "Fixed Components", "Left/Right Components"
                resx = modDatabase.$myConn.Exec("select fldsubtest,fldtanswertype,fldindex from tbltestquali where fldtestid=&1 GROUP BY fldsubtest", $rData["fldtestid"])
                If resx.Available Then
                  For Each resx
                    modLabSub.InsertLabSubTestUnit($rData["fldencounterval"], $rData["fldid"], $rData["fldtestid"], resx["fldsubtest"], resx["fldtanswertype"], resx["fldindex"], $tblpatlabsubtest)
                  Next
                Endif
            End Select
          Endif

          If modBasic.$AutoSampleLoad = "All" Then
            LoadSpecimens($rData["fldid"])
          Else If modBasic.$AutoSampleLoad = "Machine" Then
            LoadSpecimensMachine($rData["fldid"], $rData["fldtestid"])
          Endif
        Else
          Message.Warning(("Same Sample ID for different patients not possible"), ("OK"))
        Endif

      Endif
    Next

  Endif

End

Private Sub GetQUickSampling()

  Dim Row As Integer
  Dim res As Result
  Dim xallow As Boolean

  Dim xspec As String
  Dim xsampid As String
  Dim txtsampno As String
  Dim xorder As Integer
  Dim resx As Result

  For Row = 0 To GridView1.Rows.Count - 1
    $rData.MoveTo(Row)
    If Not $rData["fldsampletype"] Then

      If xspec And If xsampid Then
        If modBasic.$LabAutoSampleSuffix = "Yes" Then
          txtsampno = xsampid & modFixLab.GetTestSampleSuffix($rData["fldtestid"])
        Else
          txtsampno = xsampid
        Endif
        xallow = modLabTest.AllowSampleID($encid, txtsampno)
        If xallow = True Then
          If modFixLab.GetTestSpecimen($rData["fldtestid"]) = xspec Then
            GridView1.Select(Row)
            xorder = modFixLab.GetLabTestOrder($rData["fldtestid"])
            modLabSub.UpdateLabTestSample($rData["fldid"], txtsampno, xspec, "", "", "", "", xorder,, $tblpatlabtest)

            ''subtest
            res = modDatabase.$myConn.Exec("select fldsubtest from " & $tblpatlabsubtest & " where fldtestid=&1", $rData["fldid"])
            If res.Available = False Then
              Select modFixLab.GetLabTestOptionType($rData["fldtestid"])
                Case "Fixed Components", "Left/Right Components"
                  resx = modDatabase.$myConn.Exec("select fldsubtest,fldtanswertype,fldindex from tbltestquali where fldtestid=&1 GROUP BY fldsubtest", $rData["fldtestid"])
                  If resx.Available Then
                    For Each resx
                      modLabSub.InsertLabSubTestUnit($rData["fldencounterval"], $rData["fldid"], $rData["fldtestid"], resx["fldsubtest"], resx["fldtanswertype"], resx["fldindex"], $tblpatlabsubtest)
                    Next
                  Endif
              End Select
            Endif

            If modBasic.$AutoSampleLoad = "All" Then
              LoadSpecimens($rData["fldid"])
            Else If modBasic.$AutoSampleLoad = "Machine" Then
              LoadSpecimensMachine($rData["fldid"], $rData["fldtestid"])
            Endif
          Endif
        Endif

      Else
        xspec = modFixLab.GetTestSpecimen($rData["fldtestid"])
        If xspec Then
          xsampid = modLabSub.GetAutoIncreasingNo()
          If modBasic.$LabAutoSampleSuffix = "Yes" Then
            txtsampno = xsampid & modFixLab.GetTestSampleSuffix($rData["fldtestid"])
          Else
            txtsampno = xsampid
          Endif
          xallow = modLabTest.AllowSampleID($encid, txtsampno)
          If xallow = True Then
            GridView1.Select(Row)
            xorder = modFixLab.GetLabTestOrder($rData["fldtestid"])
            modLabSub.UpdateLabTestSample($rData["fldid"], txtsampno, xspec, "", "", "", "", xorder,, $tblpatlabtest)

            ''subtest
            res = modDatabase.$myConn.Exec("select fldsubtest from " & $tblpatlabsubtest & " where fldtestid=&1", $rData["fldid"])
            If res.Available = False Then
              Select modFixLab.GetLabTestOptionType($rData["fldtestid"])
                Case "Fixed Components", "Left/Right Components"
                  resx = modDatabase.$myConn.Exec("select fldsubtest,fldtanswertype,fldindex from tbltestquali where fldtestid=&1 GROUP BY fldsubtest", $rData["fldtestid"])
                  If resx.Available Then
                    For Each resx
                      modLabSub.InsertLabSubTestUnit($rData["fldencounterval"], $rData["fldid"], $rData["fldtestid"], resx["fldsubtest"], resx["fldtanswertype"], resx["fldindex"], $tblpatlabsubtest)
                    Next
                  Endif
              End Select
            Endif

            If modBasic.$AutoSampleLoad = "All" Then
              LoadSpecimens($rData["fldid"])
            Else If modBasic.$AutoSampleLoad = "Machine" Then
              LoadSpecimensMachine($rData["fldid"], $rData["fldtestid"])
            Endif
          Endif
        Endif

      Endif

    Endif
  Next

  If rbbar.Value = True Then
    PrintBarCodeSampling(txtsampno, xspec)
  Endif

End

Public Sub btnupdate_Click()

  If modBasic.$LabQuickMulti = "Yes" Then
    GetQUickSampling()
  Else
    GetNormalSampling()
    PrintBarCodeSampling(Trim(txtsamid.Text), cmbsamtype.Text)
  Endif
  If rbwork.Value = True Then
    btnwork_Click()
  Endif

  If rbinvoice.Value = True Then
    ShowSampledBill($encid, txtlabbill.Text)
  Else If rbencounter.Value = True Then
    ShowSampled($encid)
  Else If rbindoor.Value = True Then
    ShowSampled($encid)
  Endif
  Me.Exec(Subst("Toastify({text: '&1', duration: &2}).showToast()", "Information updated", modBasic.$BalloonDelay))

End


    Else If modBasic.$ShowDayMedReport = "Date" Then
      xx = "[ " & modReportVar.GetDateTimeReport(res["fldtime"], gb.GeneralDate) & " ]" & " : " & res["flddetail"]
    Else
      xx = res["flddetail"]
    Endif
  Else
    xx = ""
  Endif

  Return xx


      Endif
    Endif
  Next

  For k = 1 To $res3.Count
    If k < 100 Then
      If aCheckBox3[k].Value = True And If aTextBox3[k].Text Then
        res3 = modDatabase.$myConn.Create("tblpatlabsubtable")
        res3["fldtestid"] = $testID

Private Sub LoadSpecimensMachine(sIndex As Long, TestName As String)

  Dim res As Result

  If modInterface.$IntfaceTestList And If modInterface.$IntfaceTestList.Exist(TestName) Then

    res = modDatabase.$myConn.Edit($tblpatlabtest, "fldid=&1 and fldstatus=&2 and fldsampleid like &3", sIndex, "Sampled", "%")
    If res.Available Then
      If res["flduserid_start"] Then
      Else
        modInterface.InsertSampleForInterface(res["fldencounterval"], res["fldtestid"], res["fldsampleid"], res["fldsampletype"])
      Endif
      res["flduserid_start"] = modBasic.$lbluser
      If res["fldtime_start"] Then
        res["flduptime_start"] = Now()
      Else
        res["fldtime_start"] = Now()
      Endif
      res["fldcomp_start"] = modBasic.$compID
      res["fldsave_start"] = True
      res["xyz"] = True
      res.Update
    Endif

  Endif

End


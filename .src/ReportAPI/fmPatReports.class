Public Sub mnudel_Click()

  Dim sLink As String
  Dim xfile As String

  If GridView1.Selection.Count Then
    $rData.MoveTo(GridView1.Selection[0])
    If rbnew.Value = True Then
      If Message.Question(("Are you sure?"), ("No"), ("Yes")) = 2 Then

        sLink = $rData["fldlink"]
        If sLink Then
          modFTPSub.DeleteBlobFIleFTP(sLink)
          If MMain.$WebEntry = True Then
            xfile = modImage.GetBlobFileData($rData["fldrepoid"], $tblpatreport)
          Else
            xfile = modImage.GetBlobFileData($rData["fldid"], $tblpatreport)
          Endif
          If Not Exist(xfile) Then
            If MMain.$WebEntry = True Then
              modDatabase.$myConn.Delete($tblpatreport, "fldrepoid=&1 and fldsave=&2", $rData["fldrepoid"], True)
            Else
              modDatabase.$myConn.Delete($tblpatreport, "fldid=&1 and fldsave=&2", $rData["fldid"], True)
            Endif

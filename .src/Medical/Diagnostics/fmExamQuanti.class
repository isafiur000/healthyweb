Public Sub FillDrugCausingHypo()

  Dim sql As String
  Dim tbl As String
  Dim res As Result

  tbl = "tblcodebrady"
  sql = "select fldchild as col from " & tbl & " where fldparent=&1"
  res = modDatabase.$medConn.Exec(sql, lbltestname.Text)
  modFillContainer.FillListBox(lstdrughypo, res, False)

End


Public Function GetEquipmentUseDuration(encid As String, txtitem As String, sMode As String, dt1 As Date, dt2 As Date) As Float

  Dim hr As Float
  Dim sql As String
  Dim res As Result
  Dim xbol As Boolean

  If sMode = "Unit" Then
    hr = 1
  Else

    sql = "select fldfirsttime,fldsecondtime from tblpattiming where fldencounterval=&1 and fldtype=&2 and flditem=&3 and fldfirstsave=&4 and fldsecondsave=&5 and fldsecondtime>=&6 and fldsecondtime<=&7"
    If sMode = "Minute" Then
      res = modDatabase.$myConn.Exec(sql, encid, "Equipment", txtitem, True, True, modDate.StartSqlMinute(dt2), modDate.EndSqlMinute(dt2))
    Else If sMode = "Hour" Then
      res = modDatabase.$myConn.Exec(sql, encid, "Equipment", txtitem, True, True, modDate.StartSqlHour(dt2), modDate.EndSqlHour(dt2))
    Else
      res = modDatabase.$myConn.Exec(sql, encid, "Equipment", txtitem, True, True, modDate.StartSqlDate(dt2), modDate.EndSqlDate(dt2))
    Endif
    If res.Available Then
      If res.Count = 1 Then
        xbol = True
      Else If res.Count > 1 Then
        xbol = False
      Endif
      hr = GetEquipmentDurationSetting(sMode, dt1, dt2, xbol)
    Else
      hr = 0
    Endif

  Endif ''unit or time

  Return hr

End

Public Function GetWardBedUseDuration(encid As String, txtitem As String, sMode As String, dt1 As Date, dt2 As Date, xTotal As Integer) As Float

  Dim hr As Float
  Dim sql As String
  Dim res As Result
  Dim xbol As Boolean

  If modBasic.$AutoBedChargeMode = "Daily" Then
    If xTotal = 0 Then
      If dt2 < modDate.EndSqlDate(dt1) Then
        hr = 1
      Else
        hr = DateDiff(modDate.EndSqlDate(dt1), dt2, gb.Day) + 1
      Endif
    Else
      If dt2 < modDate.EndSqlDate(dt1) Then
        hr = 0
      Else
        hr = DateDiff(modDate.EndSqlDate(dt1), dt2, gb.Day)
      Endif
    Endif

  Else
    If sMode = "Unit" Then
      hr = 1
    Else
      sql = "select fldfirsttime,fldsecondtime from tblpattiming where fldencounterval=&1 and fldtype=&2 and flditem=&3 and fldfirstsave=&4 and fldsecondsave=&5 and fldsecondtime>=&6 and fldsecondtime<=&7"
      If sMode = "Minute" Then
        res = modDatabase.$myConn.Exec(sql, encid, "General Services", txtitem, True, True, modDate.StartSqlMinute(dt2), modDate.EndSqlMinute(dt2))
      Else If sMode = "Hour" Then
        res = modDatabase.$myConn.Exec(sql, encid, "General Services", txtitem, True, True, modDate.StartSqlHour(dt2), modDate.EndSqlHour(dt2))
      Else
        res = modDatabase.$myConn.Exec(sql, encid, "General Services", txtitem, True, True, modDate.StartSqlDate(dt2), modDate.EndSqlDate(dt2))
      Endif
      If res.Available Then
        If res.Count = 1 Then
          xbol = True
        Else If res.Count > 1 Then
          xbol = False
        Endif
        hr = GetBedChargeDurationSetting(sMode, dt1, dt2, xbol)
      Else
        hr = 0
      Endif
    Endif ''unit or time

  Endif

  Return hr

End

Public Function GetEquipServiceInterval(sdid As Variant) As String

  Dim res As Result
  Dim xdura As String
  Dim sMode As String
  Dim xhr As Float

  If MMain.$WebEntry = True Then  ''remoteoptimized
    res = modDatabase.$myConn.Exec("select fldencounterval,fldtype,flditem,fldfirsttime,fldsecondtime,fldhospcode from tblpattiming where fldfirstsave=&1 and fldsecondsave=&2 and fldrepoid=&3", True, True, sdid)
    If res.Available Then
      xdura = CStr(DateDiff(res["fldfirsttime"], res["fldsecondtime"], gb.Hour)) & Space(1) & "Hours"
    Endif

  Else
    res = modDatabase.$myConn.Exec("select fldencounterval,fldtype,flditem,fldfirsttime,fldsecondtime from tblpattiming where fldfirstsave=&1 and fldsecondsave=&2 and fldid=&3", True, True, sdid)
    If res.Available Then
      sMode = modNonMedical.GetBillingTargeDept(res["flditem"], res["fldtype"])
      If sMode Then
        If sMode = "Unit" Then
          xdura = "1 Unit"
        Else
          If res["fldtype"] = "Equipment" Then
            xhr = GetEquipmentDurationSetting(sMode, res["fldfirsttime"], res["fldsecondtime"], True)
          Else If res["fldtype"] = "General Services" Then
            xhr = GetBedChargeDurationSetting(sMode, res["fldfirsttime"], res["fldsecondtime"], True)
          Endif
          xdura = CStr(xhr) & Space(1) & sMode
        Endif
      Else
        xdura = CStr(DateDiff(res["fldfirsttime"], res["fldsecondtime"], gb.Hour)) & Space(1) & "Hours"
      Endif
    Endif

  Endif
  Return xdura

End


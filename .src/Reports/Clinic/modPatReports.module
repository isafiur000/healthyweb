Public Function GetPastBillingOrders(encid As String) As String

  Dim res As Result
  Dim i As Integer
  Dim xRowVal As Collection
  Dim xFinal As Variant[]
  Dim sColmList As String[]
  Dim xx As String

  xFinal = New Variant[]
  sColmList = ["", "Category", "Particulars", "QTY"]
  res = modDatabase.$myConn.Exec("select flditemtype,flditemname,flditemqty from tblpatbilling where fldencounterval=&1 and fldsave=&2 and fldstatus=&3 and fldbillno IS NULL", encid, False, "Planned")
  If res.Available Then
    i = 1
    For Each res
      xRowVal = New Collection
      xRowVal.Add(CStr(i), CStr(0))
      xRowVal.Add(res["flditemtype"], CStr(1))
      xRowVal.Add(res["flditemname"], CStr(2))
      xRowVal.Add(res["flditemqty"], CStr(3))
      xFinal.Add(xRowVal)
      i = i + 1
    Next
    xx = modString.GetRichTableStringFromVariant(sColmList, xFinal)
  Endif

  Return xx

End


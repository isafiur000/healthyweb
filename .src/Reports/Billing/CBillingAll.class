          Endif

          yqty = yqty + zqty
          ytot = ytot + ztot
          ytax = ytax + ztax
          ydisc = ydisc + zdisc
          ynet = ynet + znet
          ydepo = ydepo + zdepo
          yrecv = yrecv + zrecv
          With asx
            .Add(modString.HTMLBlankSpace(5) & res["fldcomp"])
            .Add(modReportVar.GetLocaleNumberFormat(zqty, 0))
            .Add(modReportVar.GetLocaleNumberFormat(ztot, gb.Currency))

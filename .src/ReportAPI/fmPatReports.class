''bulk delete
Public Sub mnubulkdel_Click()

  Dim res As Result
  Dim xdate As Date
  Dim sLink As String
  Dim xfile As String
  Dim xPass As String

  If cmbcategory.Text Then
    xdate = GetDateValue("Select Date of Document for Deletion", "Patient Reports", DateAdd(Now(), gb.Day, -365))
    If xdate Then

      xPass = InputPassword("Provide Root Password", "Verification", "")
      If modPassword.GetSuperVerification(modBasic.$lbluser, xPass) = True Then
        res = modDatabase.$syConn.Exec("select " & modDataRepo.SerialField() & ",fldcateg,fldtime,fldencounterval,fldencounterval,fldtitle,fldid,flddetail,fldsave,fldlink from " & $tblpatreport & " where fldcateg=&1 and fldsave=&2 and fldtime>=&3 and fldtime<=&4", cmbcategory.Text, True, modDate.StartSqlDate(xdate), modDate.EndSqlDate(xdate))                                                      ''
        If res.Available Then
          For Each res
            sLink = res["fldlink"]
            If sLink Then
              modFTPSub.DeleteBlobFIleFTP(sLink)
              If MMain.$WebEntry = True Then
                xfile = modImage.GetBlobFileData(res["fldrepoid"], $tblpatreport)
              Else
                xfile = modImage.GetBlobFileData(res["fldid"], $tblpatreport)
              Endif
              If Not Exist(xfile) Then
                If MMain.$WebEntry = True Then
                  modDatabase.$myConn.Delete($tblpatreport, "fldrepoid=&1 and fldsave=&2", res["fldrepoid"], True)
                Else
                  modDatabase.$myConn.Delete($tblpatreport, "fldid=&1 and fldsave=&2", res["fldid"], True)
                Endif
              Endif
            Else
              If MMain.$WebEntry = True Then
                modDatabase.$myConn.Delete($tblpatreport, "fldrepoid=&1 and fldsave=&2", res["fldrepoid"], True)
              Else
                modDatabase.$myConn.Delete($tblpatreport, "fldid=&1 and fldsave=&2", res["fldid"], True)
              Endif
            Endif
          Next
        Endif
      Else
        Message.Info("Incorrect Root Password", ("OK"))
      Endif

      FillGridData()
    Endif
  Endif

End

Public Sub mnuvisible_Click()

  Dim xx As String
  Dim xopt As String[] = ["Visible", "Hidden"]
  Dim res As Result

  If GridView1.Selection.Count Then
    $rData.MoveTo(GridView1.Selection[0])
    xx = InputCombo("Set Visibility", $rData["fldtitle"], xopt, $rData["flvisible"], True)
    If xx Then
      res = modDatabase.$myConn.Edit($tblpatreport, "fldid=&1", $rData["fldid"])
      If modGeneral.AllowClinicalEdit(res["flduserid"]) = True Then
        res["flvisible"] = xx
        res.Update
        FillGridData()
      Endif
    Endif
  Endif

End


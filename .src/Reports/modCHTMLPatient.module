      Endif
      If rYear["fldpatlabsubtest"] Then
        sTableSub = rYear["fldpatlabsubtest"]
      Else
        sTableSub = "tblpatlabsubtest"
      Endif
    Else
      sTableMain = "tblpatlabtest"
      sTableSub = "tblpatlabsubtest"
    Endif
  Else
    sTableMain = "tblpatlabtest"

Public Sub mnuprinthelp_Click()

  Dim xLink As String

  If modBasic.$PrintLockArchived = "Yes" Then
    Message.Warning("Printing not allowed", "OK")
  Else
    If GridView1.Selection.Count Then
      $rData.MoveTo(GridView1.Selection[0])

      If MMain.$WebEntry = True Then
        xLink = modImage.GetBlobFileData($rData["fldrepoid"], $tblpatreport)
      Else
        xLink = modImage.GetBlobFileData($rData["fldid"], $tblpatreport)
      Endif
      If xLink Then
        If modBasic.$ReportLogPrint = "Yes" Then
          modImage.SaveReportLog("Print", xLink, $rData["fldencounterval"])
        Endif

        If chkmark.Value = True Then
          If MMain.$WebEntry = True Then
            MarkPrinted($rData["fldrepoid"])
          Else
            MarkPrinted($rData["fldid"])
          Endif
        Endif
        Me.Exec("printJS('" & modPrint.GetFileWebURL(xLink) & "')")
      Endif

      If chkmark.Value = True Then
        FillGridData()
        txtencid.Text = ""
      Endif
    Endif
  Endif

End


<table class='header_table' style="text-align: left; width: 100%;" border="0" cellpadding="2" cellspacing="0" id="headtable">
  <tbody>

   <tr>
      <td style="vertical-align: top;">ENCID:{Encounter}</td>
      <td style="vertical-align: top; width: 50%;">RECEIPT NO:{InvoiceNumber}</td>
      <td colspan="1" rowspan="4" style="vertical-align: middle; width: 15%; height: 15%; text-align: center;"><img style="width: 50%; height: 50%;" alt="" src={PatientBarCode}></td>
    </tr>
    <tr>
      <td style="vertical-align: top;">NAME:{PatientName}</td>
      <td style="vertical-align: top;">TRANSACTION DATE:{BillDateTime}</td>
    </tr>
   <tr>
      <td style="vertical-align: top;">ADDRESS:{PatientAddress}</td>
      <td style="vertical-align: top;">TOTAL DEPOSIT:{CurrentDeposit}</td>
    </tr>
   <tr>

  </tbody>
</table>

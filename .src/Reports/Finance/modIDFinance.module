''------------------ tblpatbilling --------------------------
''item total sale by department
Private Function GetIDDepartSalesTotalRange($conn As Connection, sDept As String, sMode As String, bilid1 As Long, bilid2 As Long, sLocaType As String, sLocation As String, Optional sBillTable As String, Optional sBillDetail As String) As Float                                     ''

  Dim res As Result
  Dim xval As Float
  Dim RepoStr As String

  Dim xbilling As String
  Dim xbilldetail As String

  If sBillTable Then
    xbilling = sBillTable
  Else
    xbilling = "tblpatbilling"
  Endif
  If sBillDetail Then
    xbilldetail = sBillDetail
  Else
    xbilldetail = "tblpatbilldetail"
  Endif
  RepoStr = modDataRepo.GetWhereStringRepo(sLocaType, sLocation, "t1")
  res = $conn.Exec("select SUM(t1.flditemrate*t1.flditemqty) as totamt from " & xbilling & " AS t1 inner join " & xbilldetail & " AS t2 ON t1.fldbillno=t2.fldbillno where t1.flditemname in(select flditemname from tblreportgroup where fldgroup=&1) and t2.fldid>=&2 and t2.fldid<=&3 and t2.fldbilltype like &4 and t2.flditemamt>&5" & RepoStr, sDept, bilid1, bilid2, sMode, 0)                                                                    ''
  If res.Available Then
    If res["totamt"] Then
      xval = res["totamt"]
    Else
      xval = 0
    Endif
  Else
    xval = 0
  Endif
  Return xval

End


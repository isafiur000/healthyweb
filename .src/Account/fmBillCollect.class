    If xState = "All" Then
      If rbwaiting.Value = True Then
        xPath = modReportBilling.GetGroupWiseHorizontalCollection(dtfromdate.Value, dttodate.Value, "Waiting", $tblpatbilling, $tblpatbilldetail, $tbltempbilldetail)

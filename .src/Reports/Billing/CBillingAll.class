            .Add(modReportVar.GetLocaleNumberFormat(zdepo, gb.Currency))
          End With
          $BillingReport.Add(asx)
          asx.Clear()
        Endif
      Next

      xqty = xqty + yqty
      xtot = xtot + ytot
      xtax = xtax + ytax
      xdisc = xdisc + ydisc
      xnet = xnet + ynet
      xdepo = xdepo + ydepo
      xrecv = xrecv + yrecv
      With asx

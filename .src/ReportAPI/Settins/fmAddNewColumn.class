Private Sub ColumnTableValue() As String

  Dim xList As String[]
  Dim xval As String
  Dim xxx As String

  xList = modMedicine.GetMedVariables()
  xval = InputCombo("Select Patient Variable", "Clinic", xList, "", False)
  If xval Then
    xxx = "{$ColumnTable:" & xval & "$}"
  Endif

  Return xxx

End

